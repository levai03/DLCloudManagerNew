<UserControl x:Class="PluginProject.PluginPanelControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PluginProject"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Background="White">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="169,122,0,0" TextWrapping="Wrap" Text="{Binding Path=Path}" VerticalAlignment="Top" Width="379"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="169,188,0,0" TextWrapping="Wrap" Text="{Binding Path=Name1}" VerticalAlignment="Top" Width="148"/>
        <Label Content="Path:" HorizontalAlignment="Left" Height="23" Margin="129,122,0,0" VerticalAlignment="Top" Width="30"/>
        <Label Content="Name:" HorizontalAlignment="Left" Height="23" Margin="116,188,0,0" VerticalAlignment="Top" Width="43"/>
        <RadioButton IsChecked="{Binding Path=Comp}" Content="Compress" HorizontalAlignment="Left" Height="18" Margin="265,237,0,0" VerticalAlignment="Top" Width="72"/>
        <Button Click="Button_Click" Content="OK" HorizontalAlignment="Left" Height="22" Margin="254,268,0,0" VerticalAlignment="Top" Width="34"/>

    </Grid>
</UserControl>
